{
    "name": "react-store",
    "version": "0.0.0",
    "license": "MIT",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./persist": {
            "types": "./dist/middleware/persist/index.d.ts",
            "import": "./dist/middleware/persist/index.mjs",
            "require": "./dist/middleware/persist/index.js"
        }
    },
    "scripts": {
        "build": "tsup src",
        "typecheck": "tsc --noEmit",
        "typecheck:watch": "tsc --noEmit --watch",
        "format": "pnpm -C ../../ format",
        "format:check": "pnpm -C ../../ format:check",
        "lint": "eslint .",
        "dev": "tsup src --watch"
    },
    "peerDependencies": {
        "react": "^19"
    },
    "devDependencies": {
        "@eslint/js": "^9.30.1",
        "@standard-schema/spec": "^1.0.0",
        "@types/react": "19.0.12",
        "eslint": "^9.30.1",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "globals": "^16.3.0",
        "tsup": "8.0.1",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.35.1"
    },
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/deivshon/react-store.git",
        "directory": "react-store"
    },
    "bugs": {
        "url": "https://github.com/deivshon/react-store/issues"
    },
    "homepage": "https://github.com/deivshon/react-store#readme",
    "author": {
        "name": "deivshon",
        "url": "https://github.com/deivshon"
    }
}
